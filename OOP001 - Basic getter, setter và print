
import java.util.*;
	
	class Dog{
        private String name;
        private String breed;
        private int age;
        public Dog(String name , String breed , int age ){
            this.name = name;
            this.breed = breed ; 
            this.age = age ; 
        }
        public void setName(String a){
            this.name = a ;
        }
        public void setBreed(String a){
            this.breed = a ;
        }
        public void in(){
            System.out.println( "Name:" + this.name + "--Breed:" +this.breed+ "--Age:" + this.age  );
        }
        
    }

	public class Main{
        public static void main(String[] args){
            Dog  a = new Dog("Micky" , "Husky" , 12 );
            
            a.in();
        }
    }
